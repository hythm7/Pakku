#!/usr/bin/env raku

use Test;

use Pakku::Log;
use Pakku::Config;

Pakku::Log.new: verbose => 'now';
#plan 1;

my $config-file = $*TMPDIR.add( 'pakku-config.json' ~ now.Num );

my $config = Pakku::Config.new: :$config-file;

lives-ok { $config.configure( 'new' ) }, 'Can survive creating new config';

done-testing;

